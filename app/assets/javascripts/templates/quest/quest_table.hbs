<table class="table table-hover table-bordered">
    <thead>
      <tr>
        <th>Description</th>
        <th>State</th>
        <th>Difficulty</th>
        <th>Reward</th>
        <th>Action</th>
      </tr>
    </thead>
	{{#each quests as |quest|}}
		{{#isCompleted state}}
		{{else}}
			<tr>
		        <td>{{description}}</td>
		        <td>{{state}}</td>
		        <td>{{difficulty}}</td>
		        <td>{{reward}}</td>
		        <td>
			        <button type='button' class='btn btn-primary' onclick='GM.QuestController.showQuestAssignPage({{id}})'>Assign Adventurer</button>
			    </td>
			</tr>
		{{/isCompleted}}
	{{/each}}
</table>